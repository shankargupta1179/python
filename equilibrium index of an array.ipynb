     "output_type": "stream",
     "text": [
      "1 4 6\n",
      "-1\n"
     ]
    }
   ],
   "source": [
    "a = [int(s) for s in input().split()]\n",
    "k=[]\n",
    "for i in range(0,len(a)):\n",
    "    if(sum(a[:i])==sum(a[i+1:len(a)])):\n",
    "        k.append(i)\n",
    "if len(k)==0:\n",
    "    print('-1')\n",
    "else:\n",
    "    print(min(k))      "
   ]
  }
